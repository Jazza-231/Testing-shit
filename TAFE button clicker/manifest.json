{
  "manifest_version": 2,
  "name": "Quick Clicker",
  "version": "1.0",
  "description": "Skip the annoying wait on TAFE websites.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://sms.tafe.wa.edu.au/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["*://blackboard.northmetrotafe.wa.edu.au/*"],
      "js": ["content2.js"]
    }
  ]
}
